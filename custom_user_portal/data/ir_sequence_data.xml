<odoo>
  <data noupdate="0">
    <!-- Purchase Requisition -->
    <record id="seq_purchase_requisition" model="ir.sequence">
      <field name="name">Purchase Requisition</field>
      <field name="code">purchase.requisition</field>
      <field name="prefix">PR</field>
      <field name="padding">4</field>
      <field name="implementation">no_gap</field>
    </record>
    <!-- Cash PR Sequence -->
    <record id="seq_cash_purchase_requisition" model="ir.sequence">
      <field name="name">Cash Purchase Requisition</field>
      <field name="code">cash.purchase.requisition</field>
      <field name="prefix">CPR</field>
      <field name="padding">4</field>
      <field name="implementation">no_gap</field>
    </record>
    <!-- RFQ Sequence -->
    <record id="seq_purchase_rfq" model="ir.sequence">
      <field name="name">Request for Quotation</field>
      <field name="code">purchase.order.rfq</field>
      <field name="prefix">RFQ</field>
      <field name="padding">4</field>
      <field name="implementation">no_gap</field>
    </record>
  </data>
</odoo>