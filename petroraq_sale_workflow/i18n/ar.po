#. module: petroraq_sale_workflow
#: model:mail.template,body_html:petroraq_sale_workflow.petroraq_custom_sale_email
msgid ""
"\n"
"                <p style=\"font-size:13px;\">\n"
"                    <t t-set=\"doc_name\" t-value=\"'quotation' if object.state in ('draft','sent') else 'order'\"/>\n"
"\n"
"                    Hello,<br/><br/>\n"
"\n"
"                    Your <t t-out=\"doc_name\"/>\n"
"                    <strong t-out=\"object.name\"/>\n"
"                    <t t-if=\"object.origin\">(Reference: <t t-out=\"object.origin\"/>)</t>\n"
"                    amounting to\n"
"                    <strong t-out=\"format_amount(object.final_grand_total, object.currency_id)\"/>\n"
"                    is ready for review.\n"
"\n"
"                    <br/><br/>\n"
"                    Do not hesitate to contact us.\n"
"\n"
"                    <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"                        <br/><br/>\n"
"                        <t t-out=\"object.user_id.signature\"/>\n"
"                    </t>\n"
"                </p>\n"
"            "
msgstr ""
"\n"
"                <p style=\"font-size:13px;\">\n"
"                    <t t-set=\"doc_name\" t-value=\"'عرض سعر' if object.state in ('draft','sent') else 'طلب'\"/>\n"
"\n"
"                    مرحباً،<br/><br/>\n"
"\n"
"                    <t t-out=\"doc_name\"/>\n"
"                    <strong t-out=\"object.name\"/>\n"
"                    <t t-if=\"object.origin\">(المرجع: <t t-out=\"object.origin\"/>)</t>\n"
"                    بقيمة\n"
"                    <strong t-out=\"format_amount(object.final_grand_total, object.currency_id)\"/>\n"
"                    جاهز للمراجعة.\n"
"\n"
"                    <br/><br/>\n"
"                    لا تتردد في التواصل معنا.\n"
"\n"
"                    <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"                        <br/><br/>\n"
"                        <t t-out=\"object.user_id.signature\"/>\n"
"                    </t>\n"
"                </p>\n"
"            "

